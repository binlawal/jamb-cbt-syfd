    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/jwt": "^7.2.3",
    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@jamb-cbt/shared": "*",
    "@sentry/node": "^7.91.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.1.5",
    "csv-parse": "^5.5.3",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
